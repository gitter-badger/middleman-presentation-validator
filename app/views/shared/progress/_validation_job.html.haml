%ul.fe-progress
  - validation_job.progress.each do |key, value|
    %li.fe-progress-item
      %span.fe-validation_job-progress-status-name
        = validation_job.build_status key.to_sym
      - if value == true
        %i.fe-icon.fe-icon-start.fe-icon-color-grey.fe-icon-absolute-normaler.fe-icon-validation_job-progress-passed
      - elsif value == false
        %i.fe-icon.fe-icon-start.fe-icon-color-grey.fe-icon-absolute-normaler.fe-icon-validation_job-progress-failed
      - else
        %i.fe-icon.fe-icon-start.fe-icon-color-grey.fe-icon-absolute-normaler.fe-icon-validation_job-progress-not_run
    - unless key.equal? validation_job.progress.keys.last
      %i.fe-icon.fe-icon-start.fe-icon-color-grey.fe-icon-validation_job-progress-next
